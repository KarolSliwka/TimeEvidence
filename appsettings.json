{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=timeevidence.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Smtp": {
    "Host": "your_smtp_host",
    "Port": 587,
    "From": "your_email@example.com",
    "User": "your_email@example.com",
    "Pass": "your_email_password",
    "UseSsl": true
  },
  "Sms": {
    "Provider": "SMSAPI",
    "SMSAPI": {
      "AccessToken": "your_api_token",
      "From": "your_from_name"
    }
  },
  "Api": {
    "RequireAuth": true,
    "Key": "your_api_key"
  },
  "Auth": {
    "Username": "your_username",
    "Password": "your_password"
  }
}